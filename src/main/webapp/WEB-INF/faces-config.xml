<?xml version="1.0" encoding="UTF-8"?>
<faces-config
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
        version="2.0">
    <managed-bean>
        <managed-bean-name>logSearch</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.information.LogSearchController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>reco</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.intelligence.SimpleRecomendController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>alertConfig</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.AlertConfigController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>alerts</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.AlertsController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.LoginController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ourCoreTeam</managed-bean-name>
        <managed-bean-class>com.sai.slog.app.controllers.OurCoreTeamController</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
</faces-config>